<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>TestEducationBean</servlet-name>
        <servlet-class>edu.towson.test.TestEducationBean</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>edu.towson.controllers.CreateAccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestEducationBean</servlet-name>
        <url-pattern>/TestEducationBean/*</url-pattern>
    </servlet-mapping>
       <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/CreateAccount/*</url-pattern>
    </servlet-mapping>
   
</web-app>
